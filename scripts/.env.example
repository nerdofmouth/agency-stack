# Launchbox - .env.example
# Generated by Launchbox Installer
# https://nerdofmouth.com/launchbox

# Stack Configuration
STACK_NAME=Launchbox
STACK_VERSION=1.0.0
STACK_BRAND_URL=nerdofmouth.com
STACK_INSTALL_DATE=2025-04-04

# Domain Configuration
PRIMARY_DOMAIN=example.com
DASHBOARD_DOMAIN=dashboard.example.com
PORTAINER_DOMAIN=portainer.example.com
KEYCLOAK_DOMAIN=auth.example.com

# Admin Credentials
# IMPORTANT: Change these in production!
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=changeme_in_production

# Docker Configuration
DOCKER_SOCK=/var/run/docker.sock
COMPOSE_PROJECT_NAME=launchbox

# Traefik Configuration
TRAEFIK_DASHBOARD_ENABLED=true
TRAEFIK_DASHBOARD_DOMAIN=traefik.example.com
TRAEFIK_ACME_EMAIL=admin@example.com

# Builder.io Integration
BUILDER_ENABLE=false
BUILDER_API_KEY=
BUILDER_PUBLIC_API_KEY=
BUILDER_MODEL=page
BUILDER_SPACE_ID=

# Database Settings
DB_ROOT_PASSWORD=changeme_in_production
DB_BACKUP_ENABLED=true
DB_BACKUP_FREQUENCY=daily

# Client Settings
ENABLE_MULTI_CLIENT=true
CLIENT_PREFIX=client

# Monitoring Settings
NETDATA_ENABLED=true
FAIL2BAN_ENABLED=true
UPTIME_MONITOR_ENABLED=true

# Backup Settings
BACKUP_ENABLED=true
BACKUP_RETENTION_DAYS=30
BACKUP_ENCRYPTION_KEY=

# Email Settings
SMTP_ENABLED=false
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM=noreply@example.com

# Security Settings
KEYCLOAK_ENABLED=true
SSL_ENABLED=true
